includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:
    # The level 8 is the highest level
    level: 5
    
    paths:
        - app
        - bootstrap
        - config
        - routes
        - tests

    ignoreErrors:
        - '#Unsafe usage of new static#'

    excludes_analyse:
        - database/*
        - public/*
        - resources/*
        - bootstrap/cache/*
        - bootstrap/autoload.php
        - */factories/*
        - */migrations/*
        - */seeds/*
        - *.blade.php
        - *.js
        - routes/console.php

    checkMissingIterableValueType: false
    reportUnmatchedIgnoredErrors: false